                },
                {
                    "key": "Middle East (UAE) [me-central-1]",
                    "value": "me-central-1"
                }
            ],
            "default": "None",
            "mandatory": true,
            "description": "Region name to get AWS S3 Bucket from."
        },
        {
            "label": "Bucket Name",
            "key": "bucket_name",
            "type": "text",
            "default": "",
            "mandatory": true,
            "description": "Bucket Name in which the data object will be stored."
        },
        {
            "label": "Object Prefix",
            "key": "obj_prefix",
            "type": "text",
            "default": "",
            "mandatory": true,
            "description": "Object prefix for the data object name while pushing it to AWS S3."
        },
        {
            "label": "Maximum File Size (in MBs)",
            "key": "max_file_size",
            "type": "number",
            "default": 10,
            "mandatory": true,
            "description": "Maximum size of WebTx data object to be stored in the bucket. (Value should be between 0 to 100.)"
        },
        {
            "label": "Maximum Duration (in Seconds)",
            "key": "max_duration",
            "type": "number",
            "default": 30,
            "mandatory": true,
            "description": "Maximum duration after which the WebTx data object should be stored in the bucket."
        }
    ]
}